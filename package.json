{
  "name": "@oclif/plugin-warn-if-update-available",
  "description": "warns if there is a newer version of CLI released",
  "version": "3.0.2",
  "author": "Salesforce",
  "bugs": "https://github.com/oclif/plugin-warn-if-update-available/issues",
  "dependencies": {
    "@oclif/core": "^3.3.1",
    "chalk": "^5.3.0",
    "debug": "^4.1.0",
    "http-call": "^5.2.2",
    "lodash.template": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.0.2",
    "@types/chai": "^4.3.8",
    "@types/debug": "^4.1.10",
    "@types/lodash.template": "^4.5.1",
    "@types/mocha": "^10.0.3",
    "@types/node": "^18",
    "@types/semver": "^7.5.4",
    "chai": "^4.3.10",
    "commitlint": "^17.8.1",
    "eslint": "^8.52.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.6",
    "eslint-config-prettier": "^9.0.0",
    "globby": "^11.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "oclif": "^4.0.2",
    "prettier": "^3.0.3",
    "sinon": "^16.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": "./lib/index.js",
  "files": [
    "oclif.manifest.json",
    "/lib",
    "/oclif.lock"
  ],
  "homepage": "https://github.com/oclif/plugin-warn-if-update-available",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "bin": "oclif-example",
    "hooks": {
      "init": [
        "./lib/hooks/init/check-update"
      ]
    },
    "topicSeparator": " "
  },
  "repository": "oclif/plugin-warn-if-update-available",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f oclif.manifest.json oclif.lock",
    "lint": "eslint . --ext .ts",
    "postpublish": "yarn run clean",
    "posttest": "yarn lint",
    "prepare": "husky install",
    "prepublishOnly": "yarn run build && oclif lock && oclif manifest . && oclif readme",
    "pretest": "yarn build --noEmit && tsc -p test --noEmit",
    "preversion": "yarn run clean",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "type": "module"
}
