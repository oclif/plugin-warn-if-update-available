{
  "name": "@oclif/plugin-warn-if-update-available",
  "description": "warns if there is a newer version of CLI released",
  "version": "3.0.15",
  "author": "Salesforce",
  "bugs": "https://github.com/oclif/plugin-warn-if-update-available/issues",
  "dependencies": {
    "@oclif/core": "^3.26.0",
    "chalk": "^5.3.0",
    "debug": "^4.1.0",
    "http-call": "^5.2.2",
    "lodash.template": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.2.6",
    "@types/chai": "^4.3.11",
    "@types/debug": "^4.1.12",
    "@types/lodash.template": "^4.5.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18",
    "chai": "^4.4.0",
    "commitlint": "^18",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5.1.1",
    "eslint-config-oclif-typescript": "^3.1.2",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9",
    "lint-staged": "^15",
    "mocha": "^10.3.0",
    "oclif": "^4.6.1",
    "prettier": "^3.2.5",
    "shx": "^0.3.3",
    "sinon": "^16.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": "./lib/index.js",
  "files": [
    "oclif.manifest.json",
    "/lib",
    "npm-shrinkwrap.json",
    "oclif.lock"
  ],
  "homepage": "https://github.com/oclif/plugin-warn-if-update-available",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "bin": "oclif-example",
    "hooks": {
      "init": [
        "./lib/hooks/init/check-update"
      ]
    },
    "topicSeparator": " "
  },
  "repository": "oclif/plugin-warn-if-update-available",
  "scripts": {
    "build": "shx rm -rf lib && tsc",
    "clean": "shx rm -f oclif.manifest.json npm-shrinkwrap.json oclif.lock",
    "compile": "tsc",
    "lint": "eslint . --ext .ts",
    "postpack": "yarn run clean",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme && npm shrinkwrap && oclif lock",
    "prepare": "husky && yarn build",
    "pretest": "yarn build --noEmit && tsc -p test --noEmit",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "type": "module"
}
